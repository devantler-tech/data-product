{
    "Name": "Students",
    "Description": "A data product to query and manage students and courses at SDU.",
    "Version": "v0.0.1",
    "Owner": {
        "Name": "Nikolai Emil Damm",
        "Email": "nikolaiemildamm@icloud.com",
        "Organisation": "Umbraco HQ",
        "Phone": "+45 41240202",
        "Website": "www.devantler.com"
    },
    "Schemas": [
        {
            "Name": "Student",
            "Version": "v0.1",
            "Properties": [
                {
                    "Name": "Name",
                    "Type": "string"
                },
                {
                    "Name": "Age",
                    "Type": "number"
                },
                {
                    "Name": "Courses",
                    "Type": "Course[]"
                },
                {
                    "Name": "Grades",
                    "Type": "Grade[]"
                }
            ]
        },
        {
            "Name": "Course",
            "Version": "v0.1",
            "Properties": [
                {
                    "Name": "Name",
                    "Type": "string"
                },
                {
                    "Name": "Students",
                    "Type": "Student[]"
                }
            ]
        },
        {
            "Name": "Grade",
            "Version": "v0.1",
            "Properties": [
                {
                    "Name": "Course",
                    "Type": "Course"
                },
                {
                    "Name": "Value",
                    "Type": "number"
                }
            ]
        }
    ],
    "Features": {
        "Apis": [ "rest", "graphql" ],
        "DataStoreType": "relational",
        "DataStoreProvider": "sqlite"
    }
}
