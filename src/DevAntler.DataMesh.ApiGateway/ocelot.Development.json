{
    "Routes": [
        {
            "DownstreamPathTemplate": "/{everything}",
            "DownstreamScheme": "https",
            "DownstreamHostAndPorts": [
                {
                    "Host": "localhost",
                    "Port": 60001
                }
            ],
            "UpstreamPathTemplate": "/api/provisioning/{everything}",
            "UpstreamHttpMethod": [
                "Post",
                "Put",
                "Delete"
            ],
            "SwaggerKey": "provisioning",
            "DangerousAcceptAnyServerCertificateValidator": true
        }
    ],
    "SwaggerEndPoints": [
        {
            "Key": "provisioning",
            "Config": [
                {
                    "Name": "Provisioning API",
                    "Version": "v1",
                    "Url": "https://localhost:60001/swagger/v1/swagger.json"
                }
            ]
        }
    ]
}